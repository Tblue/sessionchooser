#, c-format
msgid ""
"\n"
"Your choice [%s]: "
msgstr ""
"\n"
"Ihre Wahl [%s]: "

#: ../main.c:342
#, c-format
msgid ""
"\n"
"Your choice [Text: %s]: "
msgstr ""
"\n"
"Ihre Wahl [Text: %s]: "

#: ../main.c:348
msgid ""
"\n"
"Your choice [<current shell>]: "
msgstr ""
"\n"
"Ihre Wahl [<aktuelle Shell>]: "

