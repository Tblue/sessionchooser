msgstr "  Verzeichnis für Gettext-Nachrichtenkataloge:\t%s\n"

#: ../main.c:129
#, c-format
msgid "  Session save file\t\t\t~/%s\n"
msgstr "  Sitzungsspeicherdatei:\t\t\t~/%s\n"

#: ../main.c:135
#, c-format
msgid "%s v%s (Git revision: %s)\n"
msgstr "%s v%s (Git-Revision: %s)\n"

#: ../main.c:137
#, c-format
